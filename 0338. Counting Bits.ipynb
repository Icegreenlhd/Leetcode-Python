    "class Solution(object):\n",
    "    def countBits(self, n):\n",
    "        \"\"\"\n",
    "        :type n: int\n",
    "        :rtype: List[int]\n",
    "        \"\"\"\n",
    "        count = [0 for _ in range(n+1)]\n",
    "        for i in range(1, n+1):\n",
    "            count[i] = count[i&(i-1)] + 1\n",
    "        return count"
   ]
